#!/usr/bin/env zsh

export ZSH="$HOME/.oh-my-zsh"

HYPHEN_INSENSITIVE="true"
ZSH_THEME="robbyrussell"
ZSH_CUSTOM=$ZSHRC_RUNTIME/github.com

zstyle ':omz:update' mode auto
zstyle ':omz:update' frequency 30

plugins=(
    git
    history
    dirhistory
    copybuffer
    copyfile
    web-search
    sudo
    zsh-autosuggestions
)

source $ZSH/oh-my-zsh.sh